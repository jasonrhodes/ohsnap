{
  "name": "ohsnap",
  "version": "1.3.0",
  "description": "Declarative snapshot testing for NodeJS APIs",
  "main": "ohsnap.js",
  "dependencies": {
    "supertest": "^3.0.0"
  },
  "devDependencies": {
    "body-parser": "^1.16.1",
    "chai": "^3.5.0",
    "express": "^4.14.1",
    "mocha": "^3.2.0",
    "proxyquire": "^1.7.11",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "tap": "^10.3.2",
    "tap-difflet": "^0.7.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3"
  },
  "scripts": {
    "test": "mocha ./test",
    "example-mocha": "mocha ./example/test/mocha.spec.js",
    "example-tape": "tape ./example/test/tape.spec.js | tap-difflet",
    "example-tap": "tap ./example/test/tap.spec.js",
    "examples": "npm run example-mocha && npm run example-tape && npm run example-tap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasonrhodes/ohsnap.git"
  },
  "keywords": [
    "snapshot",
    "testing",
    "snapshot testing",
    "supertest",
    "mocha",
    "tap",
    "tape"
  ],
  "author": "Jason Rhodes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jasonrhodes/ohsnap/issues"
  },
  "homepage": "https://github.com/jasonrhodes/ohsnap#readme"
}
